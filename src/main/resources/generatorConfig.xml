<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>


  <context id="DB2Tables" targetRuntime="MyBatis3">
    <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
        connectionURL="jdbc:mysql://localhost:3306/meet"
        userId="root"
        password="">
    </jdbcConnection>

    <javaTypeResolver >
      <property name="forceBigDecimals" value="false" />
    </javaTypeResolver>

    <javaModelGenerator targetPackage="com.example.meetfake.model" targetProject="MeetFake">
    
    </javaModelGenerator>

    <sqlMapGenerator targetPackage="com.example.meetfake.mapper.sql"  targetProject="MeetFake\src\main\resources">
   
    </sqlMapGenerator>

    <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.meetfake.mapper"  targetProject="MeetFake">
 
    </javaClientGenerator>

    <table tableName="tbl_user" domainObjectName="User" >

    </table>
    <table tableName="tbl_room" domainObjectName="Room" >

    </table>
    <table tableName="tbl_room_detail" domainObjectName="RoomDetail" >

    </table>

  </context>
</generatorConfiguration>